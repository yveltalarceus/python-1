#!/usr/bin/env python3
import colors as c
import time as t
import random as r

'''
What number do you need?
'''

n = input('Which number? ')
print('Here is your ' + n + ' multiplication tables.')
   
for count in range(12,0,-1):
    print(count, end=' x ' + n + ' =  ')
    print(int(n) * count)
    
